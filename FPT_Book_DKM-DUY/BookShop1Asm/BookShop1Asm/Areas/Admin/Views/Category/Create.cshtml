@model Category

<h1>Create new Category</h1>
<form  method="post">
  <div class="form-group">
    <label asp-for="Name">Name</label>
    <input asp-for="Name" />
  </div>
  <div class="form-group">
    <label asp-for="Description">Description</label>
    <input asp-for="Description" />
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
